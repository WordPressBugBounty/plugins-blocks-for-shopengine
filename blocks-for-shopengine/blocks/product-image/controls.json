    "shopengine_gallery_thumbs_width": {
        "control": {
            "field": "__experimentalUnitControl",
            "props": {
                "label": "Width",
                "isPressEnterToChange": true,
                "units": [
                    {
                        "value": "px",
                        "label": "px",
                        "default": 0
                    },
                    {
                        "value": "%",
                        "label": "%",
                        "default": 0
                    }
                ]
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": "100%",
            "tablet": "100%",
            "mobile": "100%"
        }
    },
    "shopengine_thumbs_border_style": {
        "control": {
            "field": "SelectControl",
            "props": {
                "label": "Border Type",
                "options": [
                    {
                        "label": "None",
                        "value": "none"
                    },
                    {
                        "label": "Solid",
                        "value": "solid"
                    },
                    {
                        "label": "Double",
                        "value": "double"
                    },
                    {
                        "label": "Dotted",
                        "value": "dotted"
                    },
                    {
                        "label": "Dashed",
                        "value": "dashed"
                    },
                    {
                        "label": "Groove",
                        "value": "groove"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": "none"
        }
    },

    "shopengine_thumbs_border_width": {
        "control": {
            "field": "RangeControl",
            "props": {
                "label": "Border Width(px)",
                "min": 0,
                "max": 200,
                "step": 1
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": 1
        }
    },
    "shopengine_image_border_radius": {
        "control": {
            "field": "RangeControl",
